        :pain_behind_the_eyes,
        :red_spots_over_body,
        :skin_rash,
        :vomiting ;
    :hasTreatment :avoid_fatty_spicy_food,
        :drink_papaya_leaf_juice,
        :keep_hydrated,
        :keep_mosquitos_away .

:Diabetes a :Disease ;
    :hasDescription "Diabetes is a disease that occurs when your blood glucose, also called blood sugar, is too high. Blood glucose is your main source of energy and comes from the food you eat. Insulin, a hormone made by the pancreas, helps glucose from food get into your cells to be used for energy."^^xsd:string ;
    :hasSymptom :Fatigue,
        :FrequentUrination,
        :IncreasedThirst ;
    :hasTreatment :DietModification,
        :Exercise,
        :InsulinTherapy .

