        :coma,
        :dark_urine,
        :fatigue,
        :high_fever,
        :joint_pain,
        :loss_of_appetite,
        :nausea,
        :stomach_bleeding,
        :vomiting,
        :yellowing_of_eyes,
        :yellowish_skin ;
    :hasTreatment :consult_doctor,
        :medication,
        :rest,
        :stop_alcohol_consumption .

