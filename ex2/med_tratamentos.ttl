        :fast_heart_rate,
        :fatigue,
        :irritability,
        :mood_swings,
        :muscle_weakness,
        :restlessness,
        :sweating,
        :weight_loss ;
    :hasTreatment :eat_healthy,
        :massage,
        :take_radioactive_iodine_treatment,
        :use_lemon_balm .

:Hypoglycemia a :Disease ;
    :hasDescription " Hypoglycemia is a condition in which your blood sugar (glucose) level is lower than normal. Glucose is your body's main energy source. Hypoglycemia is often related to diabetes treatment. But other drugs and a variety of conditions — many rare — can cause low blood sugar in people who don't have diabetes."^^xsd:string ;
    :hasSymptom :anxiety,
        :blurred_and_distorted_vision,
        :drying_and_tingling_lips,
        :excessive_hunger,
        :fatigue,
        :headache,
        :irritability,
        :nausea,
        :palpitations,
        :slurred_speech,
        :sweating,
        :vomiting ;
    :hasTreatment :check_in_pulse,
        :consult_doctor,
        :drink_sugary_drinks,
        :lie_down_on_side .

:Hypothyroidism a :Disease ;
    :hasDescription "Hypothyroidism, also called underactive thyroid or low thyroid, is a disorder of the endocrine system in which the thyroid gland does not produce enough thyroid hormone."^^xsd:string ;
    :hasSymptom :abnormal_menstruation,
        :brittle_nails,
        :cold_hands_and_feets,
        :depression,
        :dizziness,
        :enlarged_thyroid,
